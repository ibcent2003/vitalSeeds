<!DOCTYPE html>
<html lang="en-US" class="no-js">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- TITLE -->
    @{var title = ViewBag.Title + "Vital Seed - Source of happiness and inspiration to farmers";}
    <meta name="description" content="Vital Seed - Source of happiness and inspiration to farmers">
    <meta name="keywords" content="agriculture, eco, farm, food, Fresh Fruit, fruits, healthy food, landscape, organic, organic food, products, vegetables">

    <!-- FONT AWESOME ICONS LIBRARY -->
    <link rel="stylesheet" href="fonts/css/all.min.css">
    @Styles.Render("~/Content/NewFront/css/lib/bootstrap.min.css")
    <!--  FAVICON  -->
    <link rel="shortcut icon" href="images/icons/favicon.png">

    <!-- CSS LIBRARY STYLES -->

    <link rel="stylesheet" href="~/Content/NewFront/css/lib/slick.min.css">
    <link rel="stylesheet" href="~/Content/NewFront/css/lib/owl.carousel.min.css">
    <link rel='stylesheet' href="~/Content/NewFront/css/lib/flickity.min.css">
    <link rel='stylesheet' href="~/Content/NewFront/css/lib/magnific-popup.min.css">
    <link rel='stylesheet' href="~/Content/NewFront/css/lib/aos.min.css">
    <link rel='stylesheet' href="~/Content/NewFront/css/lib/navbar.css">

    <!-- CSS TEMPLATE STYLES -->
    <link rel="stylesheet" href="~/Content/NewFront/css/main.css">
    <link rel="stylesheet" href="~/Content/NewFront/css/stylesheet.css">
    <link rel="stylesheet" href="~/Content/NewFront/css/responsive.css">

    <!-- MODERNIZR LIBRARY -->
    <script src="~/Content/NewFront/js/modernizr-custom.js"></script>

</head>

<body>

    <!-- PRELOADER START -->
    <div id="loader-wrapper">
        <div class="loader">
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
            <div class="ball"></div>
        </div>
    </div>
    <!-- PRELOADER END -->

    <header>
        <!-- NAV START -->
        @Html.Partial("NewPartial/_MainMenu")
        <!-- NAV END -->
        <!--SLIDER START-->
        @Html.Partial("NewPartial/_Slider")
        <!--SLIDER END-->
    </header>

    <!-- CONTENT START -->
    <section>

        @RenderBody()
    </section>
    <!-- CONTENT END -->
    <!-- FOOTER START -->
    @Html.Partial("NewPartial/_Footer")
    <!-- FOOTER END -->
    <!--SCROLL TOP START-->
    <a href="#0" class="cd-top">Top</a>
    <!--SCROLL TOP START-->
    <!-- JAVASCRIPTS -->
    <script src='~/Content/NewFront/js/lib/jquery-3.5.1.min.js'></script>
    <script src='~/Content/NewFront/js/lib/plugins.js'></script>
    <script src='~/Content/NewFront/js/lib/bootstrap.min.js'></script>
    <script src='~/Content/NewFront/js/lib/flickity.pkgd.min.js'></script>
    <script src='~/Content/NewFront/js/lib/nav-fixed-top.js'></script>
    <script src='~/Content/NewFront/js/lib/aos.min.js'></script>
    <script src='~/Content/NewFront/js/lib/owl.carousel.min.js'></script>
    <script src='~/Content/NewFront/js/main.js'></script>
    <script src='~/Content/NewFront/js/slider.js'></script>
    <!-- JAVASCRIPTS END -->

</body>

</html>